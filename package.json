{
  "name": "reactLogbook",
  "version": "1.0.0",
  "homepage": "https://martinbagshaw.github.io/ReactLogbook/",
  "description": "my climbing logbook built with React",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll --coverage",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url https://martinbagshaw.github.io/ReactLogbook/",
    "lint": "eslint src/",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setuptests.ts"
    ]
  },
  "dependencies": {
    "d3": "^7.6.1",
    "gh-pages": "^2.2.0",
    "matchmedia-polyfill": "^0.3.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "react-spring": "^8.0.27",
    "styled-components": "^4.4.1",
    "views": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.0",
    "@types/d3": "^5.7.2",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-select": "^3.0.25",
    "@types/styled-components": "^5.1.3",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-minimal": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "react-testing-library": "^5.9.0",
    "typescript": "^4.0.2"
  }
}
